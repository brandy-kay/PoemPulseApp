import java.lang.Integer;
import kotlin.Int;

CREATE TABLE authorEntity (
     id INTEGER AS Int NOT NULL PRIMARY KEY AUTOINCREMENT,
     image TEXT,
     name TEXT NOT NULL
);

getAllAuthor:
SELECT * FROM authorEntity;

deleteAllAuthor:
DELETE FROM authorEntity;

updateAuthor:
UPDATE authorEntity SET image = ?,name = ?;

insertAuthor:
INSERT OR REPLACE
INTO authorEntity(image,name)
VALUES (?,?);